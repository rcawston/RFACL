<?xml version="1.0" encoding="utf-8" ?>
<RFACL_spec>
  <MaxScanDepth>5</MaxScanDepth>
  <PermissionsSpecs>
    <PermissionSpec>
      <Name>AdminFullUsersReadExecuteNoInherit</Name>
      <ProtectFromInheritance>true</ProtectFromInheritance>
      <PreserveInherited>false</PreserveInherited>
      <CleanExplicit>true</CleanExplicit>
      <UserGroup>
        <Name>Administrators</Name>
        <FileSystemRight>FullControl</FileSystemRight>
        <InheritanceFlag>ContainerInherit</InheritanceFlag>
        <InheritanceFlag>ObjectInherit</InheritanceFlag>
        <PropagationFlag>None</PropagationFlag>
        <AccessControlType>Allow</AccessControlType>
      </UserGroup>
      <UserGroup>
        <Name>Users</Name>
        <FileSystemRight>ReadAndExecute</FileSystemRight>
        <FileSystemRight>Read</FileSystemRight>
        <InheritanceFlag>ContainerInherit</InheritanceFlag>
        <InheritanceFlag>ObjectInherit</InheritanceFlag>
        <PropagationFlag>None</PropagationFlag>
        <AccessControlType>Allow</AccessControlType>
      </UserGroup>
    </PermissionSpec>
    <PermissionSpec>
      <Name>Inherit</Name>
      <ProtectFromInheritance>false</ProtectFromInheritance>
      <PreserveInherited>true</PreserveInherited>
      <CleanExplicit>false</CleanExplicit>
    </PermissionSpec>
    <PermissionSpec>
      <Name>AdminFullUsersWrite</Name>
      <ProtectFromInheritance>false</ProtectFromInheritance>
      <PreserveInherited>true</PreserveInherited>
      <CleanExplicit>true</CleanExplicit>
      <UserGroup>
        <Name>Administrators</Name>
        <FileSystemRight>FullControl</FileSystemRight>
        <InheritanceFlag>ContainerInherit</InheritanceFlag>
        <InheritanceFlag>ObjectInherit</InheritanceFlag>
        <PropagationFlag>None</PropagationFlag>
        <AccessControlType>Allow</AccessControlType>
      </UserGroup>
      <UserGroup>
        <Name>Users</Name>
        <FileSystemRight>Write</FileSystemRight>
        <FileSystemRight>Modify</FileSystemRight>
        <InheritanceFlag>ContainerInherit</InheritanceFlag>
        <InheritanceFlag>ObjectInherit</InheritanceFlag>
        <PropagationFlag>None</PropagationFlag>
        <AccessControlType>Allow</AccessControlType>
      </UserGroup>
    </PermissionSpec>
    <PermissionSpec>
      <Name>AdminOnly</Name>
      <ProtectFromInheritance>true</ProtectFromInheritance>
      <PreserveInherited>false</PreserveInherited>
      <CleanExplicit>true</CleanExplicit>
      <UserGroup>
        <Name>Administrators</Name>
        <FileSystemRight>FullControl</FileSystemRight>
        <InheritanceFlag>ContainerInherit</InheritanceFlag>
        <InheritanceFlag>ObjectInherit</InheritanceFlag>
        <PropagationFlag>None</PropagationFlag>
        <AccessControlType>Allow</AccessControlType>
      </UserGroup>
    </PermissionSpec>
  </PermissionsSpecs>
  <FolderSpecs>
    <Folder>
      <Path>\</Path>
      <Permission>AdminFullUsersReadExecuteNoInherit</Permission>
    </Folder>
    <Folder>
      <Path>\logs</Path>
      <Permission>AdminFullUsersWrite</Permission>
    </Folder>
    <Folder>
      <Path>\admin</Path>
      <Permission>AdminOnly</Permission>
    </Folder>
    <Folder>
      <Path>\*</Path>
      <StarDepth>1</StarDepth>
      <Permission>Inherit</Permission>
    </Folder>
    <Folder>
      <Path>\path\*\admin</Path>
      <StarDepth>-1</StarDepth>
      <Permission>AdminOnly</Permission>
    </Folder>
  </FolderSpecs>
</RFACL_spec>
